<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Res\Img\headerlogo.tif" />
    <None Remove="Res\logo1.png" />
    <None Remove="Res\logo2.png" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="Res\Img\headerlogo.tif">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
